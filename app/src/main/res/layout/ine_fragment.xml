<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="480px"
        android:layout_height="300px"
    android:background="@android:color/transparent">

    <ImageView
        android:id="@+id/img_ine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/credencial_elector"/>

    <TextView
        android:id="@+id/apellido_paterno_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="169px"
        android:layout_marginTop="93px"
        android:textSize="11px"
        android:text="JUAREZ"/>

    <TextView
        android:id="@+id/apellido_materno_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="169px"
        android:textSize="11px"
        android:layout_below="@+id/apellido_paterno_tv"
        android:text="GIL"/>

    <TextView
        android:id="@+id/nombre_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="169px"
        android:textSize="11px"
        android:layout_below="@+id/apellido_materno_tv"
        android:text="ERICK DANIEL"/>

</RelativeLayout>